import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int sayi;

        System.out.print("Bir sayi giriniz:");
        sayi= input.nextInt();

        for( int i=0; i<=sayi; i++){
            if((i%3==0)&&(i%4==0)){
                System.out.println(i);
            }
        }
    }
}

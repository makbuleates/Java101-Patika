import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        Scanner input= new Scanner(System.in);
        System.out.print("SayÄ± giriniz:");
        int sayi= input.nextInt();
        double sonuc=0;

        for(int i=1; i<=sayi; i++){
            sonuc+=(1.0/i);
        }
        System.out.println(sonuc);
    }
}
